# add new column academic
ALTER TABLE students
ADD COLUMN IF NOT EXISTS academic int;

#insert academic 
UPDATE students
SET academic = CASE identifier
    WHEN 1 THEN 76
    WHEN 2 THEN 90
    WHEN 3 THEN 62
    WHEN 4 THEN 58
    WHEN 5 THEN 74
    WHEN 6 THEN 54
    WHEN 7 THEN 72
    WHEN 8 THEN 59
    WHEN 9 THEN 60
    WHEN 10 THEN 93
    WHEN 11 THEN 75
    WHEN 12 THEN 64
    WHEN 13 THEN 59
    WHEN 14 THEN 63
    WHEN 15 THEN 63
    WHEN 16 THEN 74
    WHEN 17 THEN 87
    WHEN 18 THEN 95
    WHEN 19 THEN 73
    WHEN 20 THEN 66
    WHEN 21 THEN 70
    WHEN 22 THEN 81
    WHEN 23 THEN 76
    WHEN 24 THEN 60
    WHEN 25 THEN 72
    WHEN 26 THEN 51
    WHEN 27 THEN 84
    WHEN 28 THEN 98
    WHEN 29 THEN 54
    WHEN 30 THEN 50
    WHEN 31 THEN 85
    WHEN 32 THEN 81
    WHEN 33 THEN 95
    WHEN 34 THEN 77
    WHEN 35 THEN 63
    WHEN 36 THEN 82
    WHEN 37 THEN 78
    WHEN 38 THEN 89
    WHEN 39 THEN 99
    WHEN 40 THEN 97
    WHEN 41 THEN 66
    WHEN 42 THEN 81
    WHEN 43 THEN 54
    WHEN 44 THEN 51
    WHEN 45 THEN 62
    WHEN 46 THEN 67
    WHEN 47 THEN 70
    WHEN 48 THEN 84
    WHEN 49 THEN 96
    WHEN 50 THEN 80
    WHEN 51 THEN 54
    WHEN 52 THEN 84
    WHEN 53 THEN 55
    WHEN 54 THEN 97
    WHEN 55 THEN 99
    WHEN 56 THEN 65
    WHEN 57 THEN 56
    WHEN 58 THEN 70
    WHEN 59 THEN 68
    WHEN 60 THEN 87
    WHEN 61 THEN 77
    WHEN 62 THEN 94
    WHEN 63 THEN 70
    WHEN 64 THEN 92
    WHEN 65 THEN 94
    WHEN 66 THEN 66
    WHEN 67 THEN 62
    WHEN 68 THEN 92
    WHEN 69 THEN 98
    WHEN 70 THEN 98
    WHEN 71 THEN 95
    WHEN 72 THEN 63
    WHEN 73 THEN 62
    WHEN 74 THEN 68
    WHEN 75 THEN 61
    WHEN 76 THEN 75
    WHEN 77 THEN 96
    WHEN 78 THEN 85
    WHEN 79 THEN 83
    WHEN 80 THEN 73
    WHEN 81 THEN 80
    WHEN 82 THEN 69
    WHEN 83 THEN 68
    WHEN 84 THEN 87
    WHEN 85 THEN 91
    WHEN 86 THEN 67
    WHEN 87 THEN 88
    WHEN 88 THEN 90
    WHEN 89 THEN 68
    WHEN 90 THEN 71
    WHEN 91 THEN 75
    WHEN 92 THEN 61
    WHEN 93 THEN 75
    WHEN 94 THEN 92
    WHEN 95 THEN 71
    WHEN 96 THEN 52
    WHEN 97 THEN 75
    WHEN 98 THEN 84
    WHEN 99 THEN 98
    WHEN 100 THEN 99
    WHEN 101 THEN 84
    WHEN 102 THEN 95
    WHEN 103 THEN 90
    WHEN 104 THEN 94
    WHEN 105 THEN 82
    WHEN 106 THEN 68
    WHEN 107 THEN 68
    WHEN 108 THEN 57
    WHEN 109 THEN 88
    WHEN 110 THEN 97
    WHEN 111 THEN 85
    WHEN 112 THEN 55
    WHEN 113 THEN 91
    WHEN 114 THEN 87
    WHEN 115 THEN 66
    WHEN 116 THEN 67
    WHEN 117 THEN 96
    WHEN 118 THEN 96
    WHEN 119 THEN 86
    WHEN 120 THEN 56
    WHEN 121 THEN 72
    WHEN 122 THEN 78
    ELSE academic
END;

#add new column subject1,subject2,subject3
ALTER TABLE students
ADD COLUMN subject1 TEXT,
ADD COLUMN subject2 TEXT,
ADD COLUMN subject3 TEXT;

#Verify whether the addition is successful
SELECT identifier, academic, subject1, subject2, subject3
FROM students
LIMIT 10;